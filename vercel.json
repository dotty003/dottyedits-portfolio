{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        }
    ],
    "routes": [
        {
            "src": "/admin.html",
            "dest": "/admin.html"
        }
    ]
}